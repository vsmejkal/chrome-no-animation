{
  "name": "No Animation",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Disable CSS transitions and animations",
  "homepage_url": "https://github.com/vsmejkal/chrome-no-animation",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    }
  ]
}
